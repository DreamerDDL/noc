[
    {
        "name": "Juniper | JUNOS | Security | Authentication | Login (SYSLOG)",
        "description": "UI_LOGIN_EVENT: User 'admin' login, class 'j-super-user' [59120], ssh-connection '212.44.12.198 64797 217.128.15.26 22', client-mode 'cli'",
        "event_class__name": "Security | Authentication | Login",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Juniper\\.JUNOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "UI_LOGIN_EVENT: User '(?P<user>.+?)' login, class '.+?' \\[\\d+\\], \\S+ '(?P<ip>\\S+) \\d"
            }
        ]
    },
    
    {
        "name": "Juniper | JUNOS | Security | Authentication | Logout (SYSLOG)",
        "description": "UI_LOGOUT_EVENT: User 'noc-sa' logout",
        "event_class__name": "Security | Authentication | Logout",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Juniper\\.JUNOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "UI_LOGOUT_EVENT: User '(?P<user>.+?)' logout"
            }
        ]
    }
]