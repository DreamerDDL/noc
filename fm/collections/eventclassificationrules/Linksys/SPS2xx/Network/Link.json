[
    {
        "name": "Linksys | SPS2xx | Network | Link | Link Up (SYSLOG)",
        "description": "%LINK-I-Up:  gi0/3",
        "event_class__name": "Network | Link | Link Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Linksys\\.SPS2xx$"
            },
            {
                "key_re": "^message$",
                "value_re": "^%LINK-I-Up:  ?(?P<interface>.+)$"
            }
        ]
    },
    
    {
        "name": "Linksys | SPS2xx | Network | Link | Link Down (SYSLOG)",
        "description": "%LINK-W-Down:  gi0/3",
        "event_class__name": "Network | Link | Link Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Linksys\\.SPS2xx$"
            },
            {
                "key_re": "^message$",
                "value_re": "^%LINK-W-Down:  ?(?P<interface>.+)$"
            }
        ]
    },

    {
        "name": "Linksys | SPS2xx | Network | Link | Link Up (SNMP)",
        "description": "IF-MIB::linkUp interface has turned into UP state",
        "event_class__name": "Network | Link | Link Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Linksys\\.SPS2xx$"
            },
            {
                "key_re": "^IF-MIB::ifIndex\\.\\d+$",
                "value_re": "^(?P<interface>.+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^IF-MIB::linkUp$"
            }
        ]
    },

    {
        "name": "Linksys | SPS2xx | Network | Link | Link Down (SNMP)",
        "description": "IF-MIB::linkUp interface has turned into DOWN state",
        "event_class__name": "Network | Link | Link Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Linksys\\.SPS2xx$"
            },
            {
                "key_re": "^IF-MIB::ifIndex\\.\\d+$",
                "value_re": "^(?P<interface>.+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^IF-MIB::linkDown$"
            }
        ]
    }

]