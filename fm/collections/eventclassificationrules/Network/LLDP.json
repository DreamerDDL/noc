[
    {
        "name": "Network | LLDP | Remote Tables Change (SYSLOG)",
        "description": "LLDP-MIB::lldpRemTablesChange",
        "event_class__name": "Network | LLDP | Remote Tables Change",
        "preference": 2000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^LLDP-MIB::lldpStatsRemTablesAgeouts$",
                "value_re": "^(?P<table_ageouts>\\d+)$"
            },
            {
                "key_re": "^LLDP-MIB::lldpStatsRemTablesDeletes$",
                "value_re": "^(?P<table_deletes>\\d+)$"
            },
            {
                "key_re": "^LLDP-MIB::lldpStatsRemTablesDrops$",
                "value_re": "^(?P<table_drops>\\d+)$"
            },
            {
                "key_re": "^LLDP-MIB::lldpStatsRemTablesInserts$",
                "value_re": "^(?P<table_inserts>\\d+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^LLDP-MIB::lldpRemTablesChange(?:\\.\\d+)*$"
            }
        ]
    }
]