[
    {
        "name": "DLink | DxS | Chassis | PSU | PSU Failed #1 (SYSLOG)",
        "description": "WARN: Internal Power failed",
        "event_class__name": "Chassis | PSU | PSU Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)|CRIT:|CRIT\\(2\\)) (?P<name>Internal) Power failed$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Chassis | PSU | PSU Failed #2 (SYSLOG)",
        "description": "%Box-W-PS-STAT-CHNG: PS# 1 status is - not operational.",
        "event_class__name": "Chassis | PSU | PSU Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%(?:Environment|Box)-W-PS-STAT-CHNG: (?P<name>PS# \\d) status is - not operational.$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Chassis | PSU | PSU Recovered #1 (SYSLOG)",
        "description": "INFO: Internal Power is recovered",
        "event_class__name": "Chassis | PSU | PSU Recovered",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)|CRIT:|CRIT\\(2\\)) (?P<name>Internal) Power is recovered$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Chassis | PSU | PSU Recovered #2 (SYSLOG)",
        "description": "%Box-W-PS-STAT-CHNG: PS# 1 status changed - operational.",
        "event_class__name": "Chassis | PSU | PSU Recovered",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%(?:Environment|Box)-W-PS-STAT-CHNG: (?P<name>PS# \\d) status changed - operational.$"
            }
        ]
    }
]
