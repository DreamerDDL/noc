[
    {
        "name": "DLink | DxS | Security | Authentication | SNMP Authentication Failure (SYSLOG)",
        "description": "INFO: SNMP request received from 172.16.2.2 with invalid community string!",
        "event_class__name": "Security | Authentication | SNMP Authentication Failure",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(INFO:|INFO\\(6\\)) SNMP request received from (?P<ip>\\S+) with invalid community string"
            }
        ]
    },
    
    {
        "name": "DLink | DxS | Security | Authentication | Login (SYSLOG)",
        "description": "INFO: Successful login through Telnet (Username: noc, IP: 172.16.7.56)",
        "event_class__name": "Security | Authentication | Login",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(INFO:|INFO\\(6\\)) Successful login through (Web|Telnet|SSH) \\(Username: (?P<user>.+?), IP: (?P<ip>\\S+)\\)"
            }
        ]
    },
    
    {
        "name": "DLink | DxS | Security | Authentication | Logout (SYSLOG)",
        "description": "INFO: Logout through Telnet (Username: noc, IP: 172.16.7.56)",
        "event_class__name": "Security | Authentication | Logout",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(INFO:|INFO\\(6\\)) Logout through (Web|Telnet|SSH) \\(Username: (?P<user>.+?), IP: (?P<ip>\\S+)\\)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Security | Authentication | Session timed out (SYSLOG)",
        "description": "INFO: Web session timed out (Username: grant, IP: 172.16.0.4)",
        "event_class__name": "Security | Authentication | Logout",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(INFO:|INFO\\(6\\)) (Web|Telnet|SSH) session timed out \\(Username: (?P<user>.+?), IP: (?P<ip>\\S+)\\)$"
            }
        ]
    }

]
