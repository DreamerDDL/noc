[
    {
        "name": "DLink | DxS | Network | Storm Control | Broadcast Storm Detected #1 (SYSLOG)",
        "description": "WARN(4) Port 22 Broadcast storm is occurring",
        "event_class__name": "Network | Storm Control | Broadcast Storm Detected",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)) Port (?P<interface>.+) Broadcast storm is occurring"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Storm Control | Broadcast Storm Cleared #1 (SYSLOG)",
