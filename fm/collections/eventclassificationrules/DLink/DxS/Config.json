[
    {
        "name": "DLink | DxS | Config | Config Changed #1 (SYSLOG)",
        "description": "INFO: Configuration saved to flash (Username: admin, IP: <ip>)",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(INFO:|INFO\\(6\\)) Configuration (and log )*saved to flash \\(Username: (?P<user>.+), IP: \\S+.+\\)$"
            }
        ]
    },
    
