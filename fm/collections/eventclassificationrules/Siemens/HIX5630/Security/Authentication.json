[
    {
        "name": "Siemens | HIX5630 | Security | Authentication | Authentication Failed (SYSLOG)",
        "description": "invalid password for `root' on",
        "event_class__name": "Security | Authentication | Authentication Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Siemens\\.HIX5630$"
            },
            {
                "key_re": "^message$",
                "value_re": "invalid password for `(?P<user>\\S+)' on `ttyp\\d' from `(?P<ip>\\S+)'$"
            }
        ]
    },

    {
        "name": "Siemens | HIX5630 | Security | Authentication | Login (SYSLOG)",
        "description": " root login  on",
        "event_class__name": "Security | Authentication | Login",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Siemens\\.HIX5630$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?P<user>\\S+) login  on `ttyp\\d' from `(?P<ip>\\S+)'$"
            }
        ]
    }
]
