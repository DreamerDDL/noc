[
    {
        "name": "NOC | Periodic | Periodic OK",
        "description": "Periodic task has been completed successfully",
        "event_class__name": "NOC | Periodic | Periodic OK",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^status$",
                "value_re": "^success$"
            },
            {
                "key_re": "^source$",
                "value_re": "^system$"
            },
            {
                "key_re": "^task$",
                "value_re": "^(?P<task>\\S+)$"
            },
            {
                "key_re": "^type$",
                "value_re": "^periodic status$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^NOC\\.SAE$"
            }
        ]
    },

    {
        "name": "NOC | Periodic | Periodic Failed",
        "description": "Periodic task has been failed",
        "event_class__name": "NOC | Periodic | Periodic Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^status$",
                "value_re": "^failure$"
            },
            {
                "key_re": "^source$",
                "value_re": "^system$"
            },
            {
                "key_re": "^task$",
                "value_re": "^(?P<task>\\S+)$"
            },
            {
                "key_re": "^type$",
                "value_re": "^periodic status$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^NOC\\.SAE$"
            }
        ]
    },

    {
        "name": "NOC | Periodic | Probe Ok",
        "description": "NOC probe ok",
        "event_class__name": "NOC | Periodic | Periodic OK",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^system$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^NOC\\.SAE$"
            },
            {
                "key_re": "^type$",
                "value_re": "^(?P<task>pm probe)$"
            },
            {
                "key_re": "^message$",
                "value_re": "^OK$"
            }
        ]
    },

    {
        "name": "NOC | Periodic | Periodic Failed | PM Failed",
        "description": "PM Failed",
        "event_class__name": "NOC | Periodic | Periodic Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^system$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^NOC\\.SAE$"
            },
            {
                "key_re": "^message$",
                "value_re": "^Failed .+$"
            },
            {
                "key_re": "^type$",
                "value_re": "^(?P<task>pm probe)$"
            }
        ]
    }
]
