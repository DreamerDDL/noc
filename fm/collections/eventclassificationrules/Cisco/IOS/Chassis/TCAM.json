[

    {
        "name": "Cisco | IOS | Chassis | TCAM | TCAM Error #1 (SYSLOG)",
        "description": "%CFIB-SP-7-CFIB_EXCEPTION: FIB TCAM exception, Some entries will be software switched",
        "event_class__name": "Chassis | TCAM | TCAM Error",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%.+_EXCEPTION: (?P<reason>.+ TCAM exception.+)"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | TCAM | TCAM Error #2 (SYSLOG)",
        "description": "%HARDWARE-1-TCAM_ERROR: Found error in HFTM TCAM Space and not able to recover the error",
        "event_class__name": "Chassis | TCAM | TCAM Error",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%HARDWARE-1-TCAM_ERROR: (?P<reason>.+)"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | TCAM | TCAM entry capacity exceeded #1 (SYSLOG)",
        "description": "%QM-4-TCAM_ENTRY: Hardware TCAM entry capacity exceeded",
        "event_class__name": "Chassis | TCAM | TCAM Entry Capacity Exceeded",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%(?:Q|F)M-4-TCAM_ENTRY: (?P<reason>.+ capacity exceeded)"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | TCAM | TCAM entry capacity exceeded #2 (SYSLOG)",
        "description": "%EARL_NETFLOW-SP-STDBY-4-TCAM_THRLD: Netflow TCAM threshold exceeded, TCAM Utilization [99%]",
        "event_class__name": "Chassis | TCAM | TCAM Entry Capacity Exceeded",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%EARL_NETFLOW-SP(-STDBY)?-4-TCAM_THRLD: (?P<reason>.+ threshold exceeded(, TCAM Utilization \\[\\d+%\\])?)"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | TCAM | TCAM entry capacity exceeded #3 (SYSLOG)",
        "description": "%PLATFORM_UCAST-4-PREFIX: One or more, more specific prefixes could not be programmed into TCAM and are being covered by a less specific prefix, and the packets may be software forwarded",
        "event_class__name": "Chassis | TCAM | TCAM Entry Capacity Exceeded",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%PLATFORM_UCAST-4-PREFIX: (?P<reason>.+could not be programmed into TCAM.+)"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | TCAM | TCAM entry capacity exceeded #4 (SYSLOG)",
        "description": "%MLSCEF-DFC8-4-FIB_EXCEPTION_THRESHOLD: Hardware CEF entry usage is at 95% capacity for IPv4 unicast protocol.",
        "event_class__name": "Chassis | TCAM | TCAM Entry Capacity Exceeded",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%MLSCEF-.+-4-FIB_EXCEPTION_THRESHOLD: (?P<reason>.+)"
            }
        ]
    }
]
