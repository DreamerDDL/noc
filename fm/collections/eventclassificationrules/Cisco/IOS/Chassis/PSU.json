[
    {
        "name": "Cisco | IOS | Chassis | PSU | PSU Failed #1 (SYSLOG)",
        "description": "15w5d: %PLATFORM_ENV-1-DUAL_PWR: Faulty internal power supply A detected",
        "event_class__name": "Chassis | PSU | PSU Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%PLATFORM_ENV-1-DUAL_PWR: Faulty internal power supply (?P<name>\\S+) detected$"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Chassis | PSU | PSU Failed #2 (SYSLOG)",
