            },
            {
                "key_re": "^message$",
                "value_re": "%SYS-0-MOD_TEMP(?:MINOR|MAJOR)RECOVER:\\s*Module (?P<name>\\S+) (?P<threshold>minor|major) temperature threshold recovered"
            }
        ]
    },

