[
    {
        "name": "Cisco | IOS | Environment | Temperature Out of Range #1 (SYSLOG)",
        "description": "%SYS-0-MOD_TEMPMAJORFAIL: Module 2 major temperature threshold exceeded",
        "event_class__name": "Environment | Temperature Out of Range",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%SYS-0-MOD_TEMP(?:MINOR|MAJOR)FAIL:\\s*Module (?P<name>\\S+) (?P<threshold>minor|major) temperature threshold exceeded"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Environment | Temperature Returned to Normal Range #1 (SYSLOG)",
