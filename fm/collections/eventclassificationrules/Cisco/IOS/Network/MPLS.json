[
    {
        "name": "Cisco | IOS | Network | MPLS | LDP Neighbor Up (SYSLOG)",
        "description": "%LDP-5-NBRCHG: LDP Neighbor 172.16.0.1:0 is UP",
        "event_class__name": "Network | MPLS | LDP Neighbor Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$" 
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$" 
            },
            {
                "key_re": "^message$",
                "value_re": "%LDP-5-NBRCHG: LDP Neighbor (?P<neighbor>\\S+):\\d+.+is UP( \\((?P<reason>.+)\\))?$"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Network | MPLS | LDP Neighbor Down (SYSLOG)",
