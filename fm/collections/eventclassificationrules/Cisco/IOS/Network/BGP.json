[
    {
        "name": "Cisco | IOS | Network | BGP | Established (SYSLOG)",
        "description": "%BGP-5-ADJCHANGE: neighbor 10.110.0.60 Up",
        "event_class__name": "Network | BGP | Established",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%BGP-5-ADJCHANGE: neighbor (?P<peer>\\S+)( vpn vrf (?P<vrf>\\S+))? Up"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Network | BGP | Peer Down #1 (SYSLOG)",
        "description": "%BGP-5-ADJCHANGE: neighbor 10.110.0.83 Down BGP Notification sent",
        "event_class__name": "Network | BGP | Peer Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%BGP-5-ADJCHANGE: neighbor (?P<peer>\\S+)( vpn vrf (?P<vrf>\\S+))? Down"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Network | BGP | Peer Down #2 (SYSLOG)",
        "description": "%ROUTING-BGP-5-ADJCHANGE : neighbor 1.1.1.1 Down - RR client configuration changed (VRF: default) (AS: 1234)",
        "event_class__name": "Network | BGP | Peer Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%ROUTING-BGP-5-ADJCHANGE : neighbor (?P<peer>.+) Down - (?P<reason>.+) \\(VRF: (?P<vrf>\\S+)\\)\\s\\(AS:\\s(?P<as>\\d+)\\)"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Network | BGP | Peer State Changed #1 (SNMP)",
        "description": "CISCO-BGP4-MIB::ciscoBgp4MIB",
        "event_class__name": "Network | BGP | Peer State Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerState\\.\\S+$",
                "value_re": "^(?P<to_state__enum__bgp_state>\\S+)$"
            },
            {
                "key_re": "^CISCO-BGP4-MIB::cbgpPeerPrevState\\.(?P<peer>\\S+)$",
                "value_re": "^(?P<from_state__enum__bgp_state>\\S+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^CISCO-BGP4-MIB::ciscoBgp4MIB$"
            }
        ]
    },
    
