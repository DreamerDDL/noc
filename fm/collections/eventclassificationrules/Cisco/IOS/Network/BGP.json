[
    {
        "name": "Cisco | IOS | Network | BGP | Established (SYSLOG)",
        "description": "%BGP-5-ADJCHANGE: neighbor 10.110.0.60 Up",
        "event_class__name": "Network | BGP | Established",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%BGP-5-ADJCHANGE: neighbor (?P<peer>\\S+) Up"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Network | BGP | Peer Down (SYSLOG)",
        "description": "%BGP-5-ADJCHANGE: neighbor 10.110.0.83 Down BGP Notification sent",
        "event_class__name": "Network | BGP | Peer Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%BGP-5-ADJCHANGE: neighbor (?P<peer>\\S+) Down (BGP Notification sent|Peer closed the session)"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Network | BGP | Peer State Changed (SNMP)",
        "description": "CISCO-BGP4-MIB::ciscoBgp4MIB",
        "event_class__name": "Network | BGP | Peer State Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerState\\.\\S+$",
                "value_re": "^(?P<to_state>\\S+)$"
            },
            {
                "key_re": "^CISCO-BGP4-MIB::cbgpPeerPrevState\\.(?P<peer>\\S+)$",
                "value_re": "^(?P<from_state>\\S+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^CISCO-BGP4-MIB::ciscoBgp4MIB$"
            }
        ]
    }
]