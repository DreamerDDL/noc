            },
            {
                "key_re": "^message$",
                "value_re": "%ROUTING-BGP-5-ADJCHANGE : neighbor (?P<peer>.+) Down - (?P<reason>.+) \\(VRF: (?P<vrf>\\S+)\\)\\s+\\(AS:\\s+(?P<as>\\d+)\\)"
