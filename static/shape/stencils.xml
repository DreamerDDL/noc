<shapes>
    <shape name="router" aspect="fixed" w="77" h="52">
        <background>
            <path>
                <move x="0" y="15" />
                <arc rx="38" ry="15" x="77" y="15" sweep-flag="1"/>
                <arc rx="38" ry="15" x="0" y="15" sweep-flag="1"/>
                <line x="0" y="38" />
                <arc rx="38" ry="15" x="77" y="38"/>
                <line x="77" y="15" />

                <move x="24" y="27" />
                <line x="23" y="22" />
                <line x="27" y="22" />
                <line x="33" y="16" />
                <line x="40" y="18" />
                <line x="32" y="24" />
                <line x="35" y="25" />
                <line x="24" y="27" />

                <move x="53" y="3" />
                <line x="53" y="9" />
                <line x="50" y="8" />
                <line x="44" y="13" />
                <line x="37" y="11" />
                <line x="43" y="7" />
                <line x="40" y="6" />
                <line x="53" y="3" />

            </path>
        </background>
        <foreground>
            <stroke/>
        </foreground>
    </shape>
<shape name="xor" aspect="fixed">
	<background>
        <path>
            <move x="0" y="10" />
            <arc rx="50" ry="10" x="100" y="10" sweep-flag="1"/>
            <arc rx="50" ry="10" x="0" y="10" sweep-flag="1"/>
            <line x="0" y="40"/>
            <arc rx="50" ry="10" x="100" y="40"/>
            <line x="100" y="10" />
        </path>
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="or" aspect="variable">
	<background>
		<path>
			<move x="0" y="0"/>
			<quad x1="100" y1="0" x2="100" y2="50"/>
			<quad x1="100" y1="100" x2="0" y2="100"/>
			<close/>
		</path>
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="xor1" aspect="fixed">
	<background>
		<path>
			<move x="0" y="0"/>
			<quad x1="100" y1="0" x2="100" y2="50"/>
			<quad x1="100" y1="100" x2="0" y2="100"/>
			<quad x1="50" y1="50" x2="0" y2="0"/>
			<close/>
		</path>
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="and" aspect="fixed" w="50">
	<background>
		<linejoin join="round"/>
		<path>
			<move x="0" y="0"/>
			<line x="50" y="50"/>
			<line x="0" y="100"/>
			<close/>
		</path>
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
</shapes>
